<footer id="footer" class="clearfix">
  <div>
    <p><span id="since"></span><span class="my-face">(●'◡'●)ﾉ♥</span></p>
  </div>
  <div class="social-wrapper">
    <% if (theme.social) { %>
    <% theme.social.forEach(function(value, key) { 
          if (value.url && value.slug) { %>
    <a href="<%= value.url %>" class="social <%= value.slug %>" target="_blank" rel="external">
      <i class="fa fa-<%= value.slug %> %flat-box"></i>
    </a>
    <% } 
       }); %>
    <% } %>
  </div>
</footer>