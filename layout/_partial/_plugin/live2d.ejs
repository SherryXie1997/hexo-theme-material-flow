<% if (theme.live2d.enable) { %>
<div class="waifu">
    <div class="waifu-tips" style="opacity:0;"></div>
    <div style="width:250px;height:400px;overflow:hidden;">
        <canvas id="live2d" width="300" height="400" class="live2d" style="width:300px;height:400px;opacity:0;"></canvas>
    </div>
    <div id="tools" class="waifu-tool">
        <span class="fa fa-window-close"></span>
        <!-- <span class='fa fa-image'></span> -->
        <span class='fa fa-music'></span>
        <span class='fa fa-comment'></span>
        <!-- <span class='fa fa-eraser'></span> -->
    </div>
</div>
<% } %>